{
  "name": "AntiXSLeaks",
  "version": "1.0",
  "description": "Prevent XS Leaks",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "webNavigation",
    "<all_urls>"
  ],
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
  {
    "run_at": "document_start",
    "all_frames": true,
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }
],
  "manifest_version": 2
}
