{
  "name": "AntiXSLeaks",
  "version": "1.0",
  "description": "Prevent XS Leaks",
  "permissions": [
    "webRequestBlocking",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3
}
